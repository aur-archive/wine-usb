+{
+    struct reply_header __header;
+    /* VARARG(name,unicode_str); */
+};
+
+
 enum request
 {
     REQ_new_process,
@@ -5136,6 +5149,7 @@ enum request
     REQ_set_cursor,
     REQ_get_suspend_context,
     REQ_set_suspend_context,
